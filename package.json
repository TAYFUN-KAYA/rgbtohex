{
  "name": "rgbtohex",
  "displayName": "RGBTOHEX",
  "description": "convert color",
  "publisher": "tayfun-engineer",
  "icon": "media/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/TAYFUN-KAYA/rgbtohex.git"
  },
  "keywords": [
    "rgbtohex",
    "rgb to hex"
  ],
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "activationEvents": [
    "onCommand:myextension.askquestion",
    "onView:myextension-sidebar"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "myextension-sidebar-view",
          "title": "My Extension",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "myextension-sidebar-view": [
        {
          "type": "webview",
          "id": "myextension-sidebar",
          "name": "MyExtension",
          "icon": "media/icon.svg",
          "contextualTitle": "MyExtension"
        }
      ]
    },
    "commands": [
      {
        "command": "rgbtohex.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.82.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6",
    "ts-loader": "^9.4.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "@vscode/test-electron": "^2.3.4"
  }
}
